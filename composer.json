        "psr-4": {
            "Arhamlabs\\Authentication\\": "src/"
        },
        "psr-0": {
            "interface": "interface/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arhamlabs\\Authentication\\AuthenticationServiceProvider"
            ]
        }
